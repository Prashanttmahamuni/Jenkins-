pipeline {
    agent any

    stages {
        stage('Pull') {
            steps {
                git branch: 'main', url: 'https://github.com/Prashanttmahamuni/Jenkins-.git'
            }
        }

        stage('Build') {
            steps {
                echo 'Build succeed'
            }
        }

        stage('Test') {
            steps {
                echo 'Test succeed'
            }
        }

        stage('Deploy') {
            steps {
                echo 'Deploy succeed'
            }
        }
    }
}
